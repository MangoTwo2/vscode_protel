{
  "comments": {
    "lineComment": "%"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "<", "close": ">" },
    { "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["<", ">"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*;\\s*#?region",
      "end": "^\\s*;\\s*#?endregion"
    }
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
} 